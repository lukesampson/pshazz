{
    "plugins": [ "git", "hg", "ssh", "z", "aliases" ],
    "prompt": [
        [ "cyan", "", "$dir" ],
        [ "red", "", " $git_branch$git_dirty" ],
        [ "red", "", " $hg_branch$hg_dirty" ],
        [ "green", "", " `$" ]
    ],
    "git": {
        "prompt_dirty": "*"
    },
    "hg": {
        "prompt_dirty": "*"
    },
    "aliases": {
        "rm": [ "help", "man" ],
        "add": {
            "help($name)": "get-help $name -detailed | less",
            "man": "help"
        }
    }
}
