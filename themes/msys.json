{
    "plugins": [ "git", "hg", "ssh", "z", "aliases" ],
    "prompt": [
        [ "darkgreen", "", "$user@$hostname" ],
        [ "darkyellow", "", " $path" ],
        [ "", "", " $git_lbracket$git_branch" ],
        [ "red", "", "$git_dirty" ],
        [ "", "", "$git_rbracket"],
        [ "", "", " $hg_lbracket$hg_branch$hg_bookmark" ],
        [ "red", "", "$hg_dirty" ],
        [ "", "", "$hg_rbracket" ],
        [ "", "", "`n$" ]
    ],
    "git": {
        "prompt_dirty": "*",
        "prompt_lbracket": "[",
        "prompt_rbracket": "]"
    },
    "hg": {
        "prompt_dirty": "*",
        "prompt_lbracket": "[",
        "prompt_rbracket": "]"
    }
}
